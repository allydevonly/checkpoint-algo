def sum_of_distinct_elements(set1, set2):
    distinct_elements = set()
    
    # Add elements from set1 to distinct_elements and sum
    for elem in set1:
        if elem not in distinct_elements:
            distinct_elements.add(elem)
            sum += elem
    
    # Add elements from set2 to distinct_elements and sum
    for elem in set2:
        if elem not in distinct_elements:
            distinct_elements.add(elem)
            sum += elem
    
    return sum

# Example sets
set1 = [3, 1, 7, 9]
